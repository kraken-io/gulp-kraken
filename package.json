{
  "name": "gulp-kraken",
  "version": "1.0.0",
  "description": "Gulp plugin to optimize all your images with the powerful Kraken.io API",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kraken-io/gulp-kraken"
  },
  "scripts": {
    "test": "jest"
  },
  "files": [
    "index.js"
  ],
  "author": {
    "name": "Eren Burak Alic",
    "email": "erenalic@nekkra.com"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!chalk)",
      "/node_modules/(?!pretty-bytes)"
    ]
  },
  "keywords": [
    "kraken",
    "kraken-io",
    "krakenio",
    "image",
    "img",
    "picture",
    "photo",
    "png",
    "jpg",
    "jpeg",
    "gif",
    "minify",
    "minifier",
    "compress"
  ],
  "engine": {
    "node": ">=14"
  },
  "dependencies": {
    "fancy-log": "^2.0.0",
    "kleur": "^4.1.5",
    "kraken": "^1.0.1",
    "pretty-bytes": "^6.1.1",
    "request": "^2.72.0",
    "through2-concurrent": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "mocha": "*",
    "vinyl": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/kraken-io/gulp-kraken/issues"
  },
  "homepage": "https://kraken.io"
}
